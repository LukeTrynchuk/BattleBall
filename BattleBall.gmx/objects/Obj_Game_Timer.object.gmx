        draw_set_font(Main_Menu_Font);
        draw_sprite_ext(TEMP_Currency, 0, cur_x, cur_y + 200, 0.3, 0.3, 0, c_white, 1);
        draw_text(cur_x + 25, cur_y + 190, "x" + string(global.MoneyWinIncrease));
        
        draw_sprite_ext(TEMP_Currency, 0, cur_x2, cur_y + 200, 0.3, 0.3, 0, c_white, 1);
        draw_text(cur_x2 + 25, cur_y + 190, "x" + string(global.MoneyLoseIncrease));  
